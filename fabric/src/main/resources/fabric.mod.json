{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"${mod_authors}"
	],
	"contact": {
		"homepage": "${mod_url_home}",
		"issues": "${mod_url_issue}",
		"sources": "${mod_url_home}",
		"email": "leawind@yeah.net"
	},
	"license": "${mod_license}",
	"icon": "${mod_icon}",
	"environment": "client",
	"entrypoints": {
		"client": [
			"${mod_group_id}.fabric.ThirdPersonFabric"
		]
	},
	"mixins": [
		"${mod_id}.mixins.json",
		"${mod_id}-common.mixins.json"
	],
	"depends": {
		"fabric": "*",
		"minecraft": ">=${minecraft_version}",
		"architectury": ">=${architectury_version}"
	},
	"recommends": {
		"cloth-config2": ">=${cloth_config_api_version}",
		"yet_another_config_lib_v3": ">=${yacl_version}"
	},
	"custom": {
	},
	"provides": [
		"leawind_third_person_perspective"
	]
}
