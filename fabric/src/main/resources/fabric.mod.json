{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": ["${mod_authors}"],
  "contact": {
    "homepage": "${mod_url_home}",
    "sources": "${mod_url_issue}"
  },
  "license": "${mod_license}",
  "icon": "${mod_icon}",
  "environment": "client",
  "entrypoints": {
    "client": [
      "${mod_group_id}.fabric.ThirdPersonModFabric"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=1.20.1",
    "architectury": ">=9.1.12"
  }
}
